@using Carpooling.Models.View
@model AddDriveViewModel
@{
    ViewData["Title"] = "Lägg till resa";
}

<h2>Add Drive</h2>


<form asp-action="AddDrive" asp-controller="Drives">
    <label asp-for="SSN"></label>
    <input asp-for="SSN" />
    <p></p>
    <label asp-for="Date"></label>
    <input asp-for="Date" />
    <p></p>
    <label asp-for="StartingPoint"></label>
    <input asp-for="StartingPoint" />
    <p></p>
    <label asp-for="Destination"></label>
    <input asp-for="Destination" />
    <p></p>
    <label asp-for="Passengers"></label>
    <input asp-for="Passengers" />
    <input type="submit" />
    <div asp-validation-summary="All"></div>
</form>

<h3>@ViewData["Message"]</h3>
